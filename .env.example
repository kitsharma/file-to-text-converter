# Environment Variables Configuration
# Copy this file to .env and fill in your API keys

# AI Service API Keys
OPENAI_API_KEY=your-openai-api-key-here
PERPLEXITY_API_KEY=your-perplexity-api-key-here
CLAUDE_API_KEY=your-claude-api-key-here

# Government Data APIs (All Working)
BLS_API_KEY=your-bls-api-key-here
ONET_API_USERNAME=your-onet-username
ONET_API_PASSWORD=your-onet-password
CAREERONESTOP_API_KEY=your-careeronestop-api-key
CAREERONESTOP_USER_ID=your-careeronestop-user-id
FRED_API_KEY=your-fred-api-key

# Optional Base URLs (defaults work)
BLS_API_BASE_URL=https://api.bls.gov/publicAPI/v2/timeseries/data/
ONET_API_BASE_URL=https://services.onetcenter.org/ws/
CAREERONESTOP_API_BASE_URL=https://api.careeronestop.org/v1/

# Cost Management
API_MODE=test
DAILY_COST_LIMIT=10.00
MONTHLY_COST_LIMIT=100.00

# Frontend (if using Vite)
VITE_ONET_API_USERNAME=your-onet-username
VITE_ONET_API_PASSWORD=your-onet-password